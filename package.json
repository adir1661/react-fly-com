{
  "name": "react-fly-com",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.3.1",
    "i": "^0.3.6",
    "i18next": "^17.0.6",
    "i18next-browser-languagedetector": "^3.0.1",
    "i18next-xhr-backend": "^3.0.0",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.10",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.11.4",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "set \"REACT_APP_STAGE=dev\" && react-scripts start",
    "build": "react-scripts build && rm -rf ./backend/client/client/dist && mv build ./backend/client/client/dist",
    "predeploy":"set \"REACT_APP_STAGE=dev\" && npm run build",
    "deploy":"cd ./backend/client && sls client deploy --stage dev ",
    "postdeploy":"cd ../..",
    "predeploy:prod": "set \"REACT_APP_STAGE=prod\" && npm run build",
    "deploy:prod": "cd ./backend/client && sls client deploy --stage prod ",
    "postdeploy:prod": "cd ../..",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
